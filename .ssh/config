Compression yes
ControlMaster auto
ControlPath ~/.ssh/ctrl-%r@%h:%p.sock
ControlPersist 600
ServerAliveInterval 60
ServerAliveCountMax 10
User Fleshgrinder

Host github.com
    IdentityFile ~/.ssh/id.github
    IdentitiesOnly yes

Host gitlab.com
    IdentityFile ~/.ssh/id.gitlab
    IdentitiesOnly yes

Host nas
    CheckHostIP no
    HostName 192.168.178.3
    Port 2020
