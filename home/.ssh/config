AddKeysToAgent yes
UseKeychain yes
Compression yes
# https://puppet.com/blog/speed-up-ssh-by-reusing-connections
ControlMaster auto
ControlPath ~/.ssh/sockets/%r@%h-%p
ControlPersist 600
ServerAliveInterval 60
ServerAliveCountMax 10
User Fleshgrinder

Host nas
    CheckHostIP no
    HostName fleshgrinder.synology.me
    Port 2020

Host github.com
    IdentityFile ~/.ssh/github.key
    IdentitiesOnly yes
